#Nhap ten
print("Hi")
while True:
    ten = input("Moi ban nhap ten vao day: ")
    a = True
    for i in range(1, 10):
        if str(i) in ten:
            print("Moi ban nhap lai")
            a = False
            break
    if a == True:
        print("Ten cua ban la", ten)
        break
    
#So lon 13
print("Hi")
so = input("Nhap so ban muon vao day: ")
if int(so) > 13:
    print("So lon hon 13")
else:
    print("So be hon 13")
